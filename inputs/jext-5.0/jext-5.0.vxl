			<field name="auth" access="priv" jdoc=""/>
			<field name="key" access="priv" jdoc=""/>
			<field name="tServer" access="priv" jdoc=""/>
			<field name="server" access="priv" jdoc=""/>
			<mth name="/lib/:org.jext.JextLoader.JextLoader()" access="pub" jdoc="">
